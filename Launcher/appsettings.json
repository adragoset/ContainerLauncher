{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "DisableColors": true,
      "IncludeScopes": true
    }
  },
  "AllowedHosts": "*",
  "dockerEndpoint": "npipe://./pipe/docker_engine",
  "docker": {
    "email": "",
    "username": ""
  },
  "services": {
    "testApp": {
      "capabilities": [
        "SYS_NICE",
        "SYS_RESOURCE",
        "SYS_TIME"
      ],
      "cpu": 20,
      "mem": 50000000,
      "imageName": "crccheck/hello-world",
      "imageTag": "latest",
      "env": {
        "TEST_APP": "test"
      },
      "mounts": {},
      "networkMode": "host",
      "privileged": true,
      "restartPolicy": "unless_stopped",
      "forceUpgrade": false
    }
  }
}